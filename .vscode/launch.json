  "version": "0.2.0",
  "configurations": [
    {
      "name": "iSAX generate catalog",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "${command:extension.commandvariable.file.relativeFileDotsNoExtension}",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "args": [
        "-start_year", "1994",
        "-stop_year", "2023",
        "-instrument", "omni",
        "-data_path", "/mnt/c/sw-data/nasaomnireader/",
        // "-debug", "10"
        "-histogram"
      ]
    },
    {
      "name": "iSAX experiment",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "${command:extension.commandvariable.file.relativeFileDotsNoExtension}",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "args": [
        "-word_size","4",
        "-start_date", "2018-01-01",
        "-stop_date", "2018-06-01",
        "-min_cardinality","8",
        "-max_cardinality","32",
        "-cadence", "60",
        "-chunk_size","3000",
        "-smooth_window","3000",
        "-detrend_window","18000",
        "-instrument", "omni",
        "-cache",
        "-cache_folder", "/home/jasminekobayashi/isax_cache/",
        "-transliterate", 
        "-plot_cluster"
      ]
    },
    {
      "name": "iSAX clustering iterations",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "${command:extension.commandvariable.file.relativeFileDotsNoExtension}",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "args": [
        "-word_size","5",
        "-start_date", "2018-01-01",
        "-stop_date", "2018-06-01",
        "-min_cardinality","8",
        "-max_cardinality","32",
        "-cadence", "60",
        "-chunk_size","3000",
        "-smooth_window","18000",
        "-detrend_window","3000",
        "-instrument", "omni",
        "-cache",
        "-min_samples","3",
        "-min_cluster_size","10",
        "-cache_folder", "/home/jasminekobayashi/isax_cache/",
        "-transliterate", 
        "-plot_cluster",
        "-recluster_iterations","-1",
        "-set_largest_cluster_to_noncluster"
      ]
    }
  ]
}
